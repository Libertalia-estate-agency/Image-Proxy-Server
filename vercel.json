{
    "version": 2,
    "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Content-Encoding", "value": "gzip" }
      ]
    }
    ],
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/", "dest": "/api/index.js" },
      { "src": "/convert", "dest": "/api/index.js" },
      { "src": "/convertMultiple", "dest": "/api/index.js" }
    ]
  }
  